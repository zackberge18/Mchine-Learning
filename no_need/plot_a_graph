pip install matplotlib

import matplotlib.pyplot as plt

x = [i for i in range(10)]

print(x)

y = [2*i for i in range(10)]

print(y)

plt.xlabel('x-axis')
plt.ylabel('y-axis')
plt.plot(x, y)

plt.xlabel('x-axis')
plt.ylabel('y-axis')

plt.scatter(x, y)
